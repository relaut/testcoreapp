pipeline {
    agent any 
    stages {
        stage('Starting') {
            steps {
                echo 'Building application!' 
            }
        }
	stage('Building Docker Image') {
            steps {
		node('jnlp-docker') {
			imageTag = "${projectName}:${params.ENV}.${env.BUILD_NUMBER}".toLowerCase()
			echo "Image tag is ${imageTag}"
			def password = "${env.DOCKER_PASSWORD}"
			echo "Password is ${password}"
		}
        }
}
