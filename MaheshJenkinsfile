pipeline {
    agent any 
	stages {
        stage('Starting') {
            steps {
                echo 'Building application!' 
            }
        }
			node('jnlp-docker') {
				def imageTag = "${projectName}:${params.ENV}.${env.BUILD_NUMBER}".toLowerCase()
				echo "Image tag is ${imageTag}"
				def password = "${env.DOCKER_PASSWORD}"
				echo "Password is ${password}"
			}
	}
}
